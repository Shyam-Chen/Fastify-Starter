    "@fastify/redis": "^6.1.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/under-pressure": "^8.2.0",
    "@fastify/websocket": "^7.1.2",
    "@sinclair/typebox": "^0.25.23",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.7.0",
    "fastify": "^4.13.0",
    "fastify-cloudinary": "^1.0.5",
    "fastify-i18n": "^1.1.1",
    "fastify-plugin": "^4.5.0",
