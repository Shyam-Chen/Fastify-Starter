{
  "name": "fastify-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -e \"ts,mts,json\" -x \"vite-node src/main.ts\" -w \"src/**/*\" -i \"**/__tests__/**\"",
    "build": "vite build",
    "lint": "eslint 'src/**/*.ts'",
    "unit": "vitest --dir src --coverage",
    "preview": "node dist/main.mjs",
    "e2e": "cd e2e && pnpm test",
    "meas": "cd e2e && pnpm perf",
    "mock": "cd mock/requests && pnpm dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.1.0",
    "@fastify/jwt": "^6.3.2",
    "@fastify/mongodb": "^6.0.1",
    "@fastify/multipart": "^7.2.0",
    "@fastify/redis": "^6.0.1",
    "@fastify/sensible": "^5.1.1",
    "@fastify/websocket": "^7.0.1",
    "@sinclair/typebox": "^0.24.44",
    "fastify": "^4.6.0",
    "fastify-plugin": "^4.2.1",
    "fastify-sse-v2": "^2.2.1",
    "node-polyglot": "^2.4.2"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.23.4",
    "nodemon": "^2.0.20",
    "process-envify": "^1.0.4",
    "vite": "^3.1.3",
    "vite-node": "^0.23.4",
    "vitest": "^0.23.4"
  }
}
