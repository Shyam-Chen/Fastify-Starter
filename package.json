{
  "name": "fastify-starter",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon -e \"ts,mts,json\" -x \"vite-node src/main.ts\" -w \"src/**/*\" -i \"**/__tests__/**\"",
    "build": "vite build --ssr=src/main.ts && vite build --emptyOutDir=false",
    "preview": "concurrently \"node index.mjs\" \"caddy run\"",
    "lint": "eslint 'src/**/*.ts' && tsc",
    "unit": "vitest --dir src --coverage --run",
    "e2e": "cd e2e && pnpm e2e",
    "bench": "cd benchmark && pnpm bench",
    "mock": "cd mock/requests && pnpm mock",
    "doc": "cd docs && pnpm dev"
  },
  "dependencies": {
    "@fastify/cors": "^8.2.0",
    "@fastify/jwt": "^6.5.0",
    "@fastify/mongodb": "^6.2.0",
    "@fastify/multipart": "^7.3.0",
    "@fastify/one-line-logger": "^1.1.1",
    "@fastify/redis": "^6.1.0",
    "@fastify/sensible": "^5.2.0",
    "@fastify/type-provider-typebox": "^2.4.0",
    "@fastify/websocket": "^7.1.1",
    "@sinclair/typebox": "^0.25.11",
    "bcrypt": "^5.1.0",
    "bullmq": "^3.3.4",
    "fastify": "^4.10.2",
    "fastify-cloudinary": "^1.0.5",
    "fastify-i18n": "^1.0.1",
    "fastify-plugin": "^4.4.0",
    "fastify-sse-v2": "^3.0.0",
    "ioredis": "^5.2.4",
    "nodemailer": "^6.8.0",
    "nunjucks": "^3.2.3",
    "pm2": "^5.2.2",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.6",
    "@types/nunjucks": "^3.2.1",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "@vitest/coverage-c8": "^0.23.4",
    "concurrently": "^7.5.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "mongodb-memory-server": "^8.9.5",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "process-envify": "^2.0.0",
    "typescript": "^4.8.4",
    "vite": "^3.1.3",
    "vite-node": "^0.23.4",
    "vitest": "^0.23.4"
  }
}
